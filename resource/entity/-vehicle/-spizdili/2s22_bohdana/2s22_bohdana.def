{game_entity
	(include "/properties/car.ext")
	(include "/properties/shadow/tank.inc"  scale(1.0))
	(include "/properties/selection/vehicle.inc"  scale(1.1))
	{targetSelector "cannon_rocket"}
	{collider "truck"}
	{props "vehicle" "turret_rotator" "tank" "armored_car_M" "turret" "wheeled" "with_gun" "vision_lev04" "6_wheels" "cw_artillery" "ural4320"}
	{able "emit_on_attack"}
	("armor_car_truck")


	{boarder
		{anm "driver"
			{forward	{begin	"board_Kubel_driver"} {end "seat_driver_car"} {base "doorleft"}}
			{reverse 	{end	"board_Kubel_driver" -1} {base "doorleft" -1}}
		}
		{anm "commander"
			{forward	{begin	"board_Kubel_commander"} {end "seat_passenger"} {base "doorright"}}
			{reverse 	{end	"board_Kubel_commander" -1} {base "doorright" -1}}
		}
		{anm "passenger1"
			{forward	{begin	"board_Kubel_leftpassenger"} {end "seat_passenger"} {base "doorleft1"}}
			{reverse 	{end	"board_Kubel_leftpassenger" -1} {base "doorleft1" -1}}
		}
		{anm "passenger2"
			{forward	{begin	"board_Kubel_rightpassenger"} {end "seat_passenger"} {base "doorright1"}}
			{reverse 	{end	"board_Kubel_rightpassenger" -1} {base "doorright1" -1}}
		}
	}

	("crew_2_human")
	("board_animation" place(driver) board_begin(board_big-midcar_driver) board_end(seat_driver_car) base(open1))
	("board_animation" place(commander) board_begin(board_big-midcar) board_end(seat_passenger) base(open2))

 	("crew_human" place(seat1) bone(seat01) door(emit3) animation(passenger1))
	("crew_human" place(seat2) bone(seat02) door(emit4) animation(passenger2))


	("wheel" args "wheelright1")
	("wheel" args "wheelleft1")
	("wheel" args "wheelright2")
	("wheel" args "wheelleft2")
	("wheel" args "wheelright3")
	("wheel" args "wheelleft3")
	("wheel" args "wheelright4")
	("wheel" args "wheelleft4")

	{extender "enumerator"}


	{volume "body1"
		{component "body"}
		{tags "body"}
	}
	{volume "body2"
		{component "body"}
		{tags "body"}
	}
	{volume "body3"
		{component "body"}
		{tags "body"}
	}
	{volume "body4"
		{component "body"}
		{tags "body"}
	}
	{volume "body5"
		{component "body"}
		{tags "body"}
	}

	{extender "inventory"
		{box
			{size 12 6}
			{item "ammo bullet155_USA_h fg" 20}
		}
	}
	{Weaponry
		{place "gun"
			{weapon "155mm_Bogdana" filling "ammo bullet155_USA_h fg" 1}
			{foresight "foresight1"}
			{charger "driver"}
			{gunner "driver"}

		}
	}

	{mass 28000}
	
	{Chassis
		{Locomotion ("locomotion")
			{maxspeed 80} ; Максимальна швидкість.
                        {StartTime 50} ; Розгін.
                        {BrakeTime  12.5} ; Час гальмування з максимальної швидкісті до 0 у секундах.
	                {StopTime   5.5} ; Час гальмування після вимкнення двигуна з максимальної швидкісті до 0 у секундах.
	{Gears   0.030 0.060 0.120 0.300 1.000}  ; Передні передачі.
	{RearGears  0.015 0.030 0.060 0.120 0.240}  ; Задні передачі.
        {GearSwitchDelay 1.0} 
	{maxSpeedAtMaxTurn 12}
      	{TurnRadius 6}
		}
				{FuelBag
					{volume 500 }	
					{rate	0.625 }   
					{remain 500 }
					{fuel "fuel"}
				}
		{speed
			{Normal  42}
			{Slow  40}
		}
		("car_trace" fx("tracewheel03") step(0.08) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("car_trace" fx("tracewheel03") step(0.08) wheell("wlr") wheelr("wrr") bonel("FXtraceL2") boner("FXtraceR2"))
		{DisableMoveWhileFiring}
	}



	{Extension "2S22_Bohdana.mdl"}


	
}
