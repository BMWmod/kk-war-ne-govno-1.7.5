			{forward	{end "lie_take_item_begin"}}
		}
		{anm "driver"
			{forward {end "mortar_driver_r"}}
		}
		{link "driver" "gunner"}
	}
	{Extension "switchblade600_catapult.mdl"}
	("missile_aimpoint_stansize")

{Volume "select"
