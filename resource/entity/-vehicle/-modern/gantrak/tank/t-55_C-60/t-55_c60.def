	}
	{volume "turret"
		{tags "turret"}
		{component "turret"}
	}

("armor_heavy" turret(5 5 5 5) body(102 80 45 25))

	;("armor_engine"  t(40 60 60 40))

	("shield_M1A2TUSK")  
	{extender "shield"
		{on}
	}
	{component "shieldl1" {tags "shield"}} 
 	{component "shieldr1" {tags "shield"}} 
	{component "shieldl6" {tags "shield"}}
