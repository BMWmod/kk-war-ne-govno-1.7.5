{game_entity
	(include "/properties/car.ext")
	(include "/properties/shadow/tank.inc"  scale(1.0))
	(include "/properties/selection/vehicle.inc"  scale(1.1))
	{collider "truck"}
	{props "vehicle" "remote_turret" "turret_rotator" "sam_radar" "wheeled" "amphibia" "has_apu" "6_wheels" "vision_lev05" "man_kat" }
	
	{targetSelector "missile"}
	{PatherID "amphibia"}
	{able "emit_on_attack"}
	("armor_car_truck")


	
	{boarder
		{anm "left"
			{forward	{begin	"board_tank_right"} {end "seat_gunner_stand"}}
			{reverse 	{end	"emit_tank_2_hold"}}
		}
		{anm "right"
			{forward	{begin	"board_tank_left"} {end "seat_gunner_stand"}}
			{reverse 	{end	"emit_tank_2_hold"}}
		}
	}

{placer
	{place "driver" {group "crew"} {visor "driver"}}
}
{boarder
	{door "emit1"}
	{link "emit1" "driver" {anm "left"} {forward putoff} {reverse puton}}
}

{placer
	{place "gunner" {group "crew"} {visor "main"}}
}
{boarder
	{door "emit2"}
	{link "emit2" "gunner" {anm "right"} {forward putoff} {reverse puton}}
}
{placer
	{place "charger" {group "crew"} {visor "main"}}
}
{boarder
	{door "emit1"}
	{link "emit1" "charger" {anm "right"} {forward putoff} {reverse puton}}
}
{placer
	{place "commander" {group "crew"} {visor "main"}}
}
{boarder
	{door "emit2"}
	{link "emit2" "commander" {anm "right"} {forward putoff} {reverse puton}}
}
	{sensor
		{visor "visor_main"
			{vision "vehicle_main"}
			{bone "visor2"}
		}
		{visor "visor_main"
			{vision "tank_gunner_optic_IR_3g"}
			{bone "visor1"}
		}
		{visor "visor_radar"
			{vision "radar_around_roland"}
			{bone "visor3"}
		}
	}


	{extender "inventory"
		{box
			{Size 8 4}
			{item "9m33 ammo" 6}
			{item "netting_kit" 5}
		}
	}	
	{Weaponry
		{place "gun"
			{weapon "osa_akm_x6" filling "9m33 ammo" 6}
			{foresight "foresight"}
			{charger "gunner"}
			{gunner "gunner"}
			{barrels 6}
			
			{basic}
		}
	}

  {mass 18000}
  {Chassis
	("CW_wheels"
		Mass(27.0)
		Turnradius(10.2)
		Power(360)
		Realspeed(90)
		Gamespeed(51) 
		Range(800)
		Fuelvolume(441)
	)
	

	("car_trace" fx("tracewheel02") step(0.09) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
	("car_trace" fx("tracewheel02") step(0.09) wheell("wlr") wheelr("wrr") bonel("FXtraceL2") boner("FXtraceR2"))
	{DisableMoveWhileFiring}
  }
  {props "cwfs_ext_only" "cwfs_extfuel_400"}
  
	("sam_radar" type(rus))
	("CW_amphibia" Waterline(1.3) )
	("two_headlights")
	{Extension "osa_akm.mdl"}
	("missile_aimpoint_trucksize")

	("cwts_buksir")

}

